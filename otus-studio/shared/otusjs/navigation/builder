/* Rota direta, sem condição a ser respeitada */
builder
    .createRoute()
    .from('CID')
    .to('NID');

/* Rota que ocorre sob determinada condição */
builder
    .createRoute()
    .from('CID')
    .to('NID')
    .underCondition('CONDITION_A')
    .question('QID')
    .answer
    .isGreaterThan(5)
    .and()
    .answer
    .isLowerThan(10)
    .or()
    .underCondition('CONDITION_A')
    .question('BQID')
    .hasNotAnswer();
